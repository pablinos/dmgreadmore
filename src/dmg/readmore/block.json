{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "dmg/readmore",
	"version": "0.1.0",
	"title": "DMG Read More",
	"category": "widgets",
	"icon": "book-alt",
	"description": "A block that adds a 'Read More' link to a selected post",
	"example": {},
	"attributes": {
		"postId": {
			"type": "number"
		},
		"postTitle": {
			"type": "string",
			"default": ""
		},
		"postLink": {
			"type": "string",
			"default": ""
		}
	},
	"supports": {
		"html": false,
		"align": true
	},
	"textdomain": "dmgreadmore",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
